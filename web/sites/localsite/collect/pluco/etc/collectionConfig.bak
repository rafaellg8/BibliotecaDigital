			<displayItem lang="es" name="name">Búsqueda por autores</displayItem>
		</index>
		<index name="text,PLUCO.description,PLUCO.tags">
			<displayItem lang="es" name="name">Búsqueda por etiquetas</displayItem>
		</index>
